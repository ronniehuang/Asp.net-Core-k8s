          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="Get empty name" enabled="true">
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="script">String  fullJson = prev.getResponseDataAsString();
vars.put(&quot;NewName&quot;, &quot;&quot;);
Integer i=0;
for( i=1;i&lt;100000;i++)
{
	String  strName =&quot;PtTest&quot; + i.toString();
	if ( fullJson.indexOf(strName) &lt; 0 )
	{
		vars.put(&quot;NewName&quot;, strName);
		vars.put(&quot;NewValue&quot;, strName);
		break;
	}
}
for(Integer j=i+1;j&lt;100000;j++)
{
	String  strName =&quot;PtTest&quot; + j.toString();
	if ( fullJson.indexOf(strName) &lt; 0 )
	{
		vars.put(&quot;UpdateName&quot;, strName);
		vars.put(&quot;UpdateValue&quot;, strName);
		break;
	}
}</stringProp>
